prefix=/usr
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: libffmpegthumbnailer
Description: Generates thumbnails for movie files
Version: 2.1.1
Libs: -L${libdir} -lffmpegthumbnailer
Libs.private: -L -lavcodec -lavutil -lavfilter -lavformat /usr/lib/libjpeg.so /usr/lib/libpng.so /usr/lib/libz.so
Cflags: -I${includedir}
