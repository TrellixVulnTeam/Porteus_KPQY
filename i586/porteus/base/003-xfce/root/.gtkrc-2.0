style "xfdesktop-icon-view" {
    XfdesktopIconView::label-alpha = 10
    XfdesktopIconView::ellipsize-icon-labels = 1
    base[NORMAL] = "#000000"
    base[SELECTED] = "#71B9FF"
    base[ACTIVE] = "#C3C3C3"
    fg[NORMAL] = "#ffffff"
    fg[SELECTED] = "#ffffff"
    fg[ACTIVE] = "#000000"
}
widget_class "*XfdesktopIconView*" style "xfdesktop-icon-view"