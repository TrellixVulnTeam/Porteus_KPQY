prefix=/usr
exec_prefix=${prefix}
libdir=/usr/lib64
includedir=${prefix}/include

Name: LibVNCClient
Description: A library for easy implementation of a VNC client.
Version: 0.9.9
Requires:
Libs: -L${libdir} -lvncclient -lnsl -lpthread -lz -lpng -ljpeg -lresolv -lssl -lcrypto -L/usr/lib64 -lgcrypt -lgpg-error  
Cflags: -I${includedir}

