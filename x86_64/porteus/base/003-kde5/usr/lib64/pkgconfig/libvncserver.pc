prefix=/usr
exec_prefix=${prefix}
libdir=/usr/lib64
includedir=${prefix}/include

Name: LibVNCServer
Description: A library for easy implementation of a VNC server.
Version: 0.9.10
Requires:
Requires.private: zlib
Libs: -L${libdir} -lvncserver
Libs.private: -lnsl -lpthread -lz -lpng -ljpeg -lresolv -lssl -lcrypto -L/usr/lib64 -lgcrypt -lgpg-error -lgnutls  
Cflags: -I${includedir}

