#!/bin/sh
# Find and run a web browser.

where() { which $1 2>/dev/null; }
browser=`where firefox || where chromium || where chromium-ungoogled || where basilisk || where netsurf || where google-chrome || where google-chrome-stable || where opera || where midori || where konqueror || where palemoon || where netsurf || where vivaldi`

$browser $* ||  uxterm -e lynx $*


